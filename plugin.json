{
  "library": "library.js",
  "hooks": [
    {
      "hook": "filter:navigation.available",
      "method": "getNavigation"
    },
    {
      "hook": "static:app.load",
      "method": "load"
    },
    {
      "hook": "filter:post.create",
      "method": "postCreate"
    },
    {
      "hook": "filter:user.whitelistFields",
      "method": "appendUserFieldsWhitelist"
    }
  ],
  "languages": "public/language",
  "defaultLang": "en-US",
  "templates": "./public/templates"
}